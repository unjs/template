{
  "name": "package-name",
  "version": "0.0.0",
  "description": "",
  "repository": "unjs/package-name",
  "homepage": "https://github.com/unjs/package-name#readme",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint --cache --ext ts,js,mjs,cjs . && pnpm run format",
    "lint:fix": "eslint --cache --ext ts,js,mjs,cjs . --fix && pnpm run format:fix",
    "format": "prettier --check . --ignore-path .eslintignore",
    "format:fix": "prettier --check . --ignore-path .eslintignore --write",
    "prepack": "pnpm run build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.28.4",
    "changelogen": "^0.4.1",
    "eslint": "^8.34.0",
    "eslint-config-unjs": "^0.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.1",
    "vitest": "^0.28.4"
  },
  "packageManager": "pnpm@7.27.0"
}
